@article{vahed2022,
  title = {Can a {{Black-Box AI Replace Costly DMA Testing}}?\textemdash{{A Case Study}} on {{Prediction}} and {{Optimization}} of {{Dynamic Mechanical Properties}} of {{3D Printed Acrylonitrile Butadiene Styrene}}},
  shorttitle = {Can a {{Black-Box AI Replace Costly DMA Testing}}?},
  author = {Vahed, Ronak and Zareie Rajani, Hamid R. and Milani, Abbas S.},
  year = {2022},
