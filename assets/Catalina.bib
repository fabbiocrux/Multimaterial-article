@article{menon2019,
  title = {Optimization of {{Silicone 3D Printing}} with {{Hierarchical Machine Learning}}},
  author = {Menon, Aditya and P{\'o}czos, Barnab{\'a}s and Feinberg, Adam W. and Washburn, Newell R.},
