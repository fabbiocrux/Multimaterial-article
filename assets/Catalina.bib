@article{love2014,
  title = {The Importance of Carbon Fiber to Polymer Additive Manufacturing},
  author = {Love, Lonnie J. and Kunc, Vlastamil and Rios, Orlando and Duty, Chad E. and Elliott, Amelia M. and Post, Brian K. and Smith, Rachel J. and Blue, Craig A.},
  year = {2014},
