@article{gwamuri2014,
  title = {Reversing the {{Trend}} of {{Large Scale}} and {{Centralization}} in {{Manufacturing}}: {{The Case}} of {{Distributed Manufacturing}} of {{Customizable}} 3-{{D-Printable Self-Adjustable Glasses}}},
