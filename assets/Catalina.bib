@article{araujo2019,
  title = {The {{Digital Pharmacies Era}}: {{How 3D Printing Technology Using Fused Deposition Modeling Can Become}} a {{Reality}}},
  shorttitle = {The {{Digital Pharmacies Era}}},
  author = {Ara{\'u}jo, Maisa R. P. and {Sa-Barreto}, Livia L. and Gratieri, Tais and Gelfuso, Guilherme M. and {Cunha-Filho}, Marcilio},
