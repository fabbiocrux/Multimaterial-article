}

@article{raju2019,
  title = {A Hybrid {{PSO}}\textendash{{BFO}} Evolutionary Algorithm for Optimization of Fused Deposition Modelling Process Parameters},
  author = {Raju, Maraboina and Gupta, Munish Kumar and Bhanot, Neeraj and Sharma, Vishal S.},
  year = {2019},
