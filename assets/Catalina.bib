@article{pearce2016,
  title = {Open-{{Source Wax RepRap}} 3-{{D Printer}} for {{Rapid Prototyping Paper-Based Microfluidics}}},
  author = {Pearce, Joshua and Anzalone, Nick and Heldt, Caryn},
  year = {2016},
