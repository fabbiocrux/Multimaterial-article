}

@article{foresti2021,
  title = {{{3D Printed Masks}} for {{Powders}} and {{Viruses Safety Protection Using Food Grade Polymers}}: {{Empirical Tests}}},
  shorttitle = {{{3D Printed Masks}} for {{Powders}} and {{Viruses Safety Protection Using Food Grade Polymers}}},
  author = {Foresti, Ruben and Ghezzi, Benedetta and Vettori, Matteo and Bergonzi, Lorenzo and Attolino, Silvia and Rossi, Stefano and Tarabella, Giuseppe and Vurro, Davide and {von Zeppelin}, Didier and Iannotta, Salvatore and Zappettini, Andrea and Macaluso, Guido Maria and Miragoli, Michele and Maggio, Marcello Giuseppe and Costantino, Cosimo and Selleri, Stefano and Macaluso, Claudio},
  year = {2021},
