  author = {Wang, Chunguang and Dai, Wei and Zhang, Zishou and Mai, Kancheng},
  year = {2013},
  month = feb,
  journal = {Journal of Thermal Analysis and Calorimetry},
  volume = {111},
  number = {2},
  pages = {1585--1593},
  issn = {1572-8943},
  doi = {10.1007/s10973-012-2425-0},
  urldate = {2022-02-20},
  abstract = {The main purpose of this study is to investigate the {$\beta$}-crystallization tendency in the {$\beta$}-nucleated iPP blends. The {$\beta$}-nucleated iPP/compatibilizers blends, {$\beta$}-nucleated iPP/PET blends and its compatibilized versions with four kinds of compatibilizers (PP-g-MA, PP-g-GMA, POE-g-MA, and EVA-g-MA) were prepared by different blending ways. The effect of compatibilizers and blending ways on the non-isothermal crystallization and melting characteristics and the {$\beta$}-crystallization tendency of {$\beta$}-nucleated iPP blends were studied by differential scanning calorimetry. The relative content of the {$\beta$}-phase were characterised by the k{$\beta$}values determined on the basis of the wide angle X-ray diffractogram. The results indicated that the {$\beta$}-crystallization tendency of {$\beta$}-nucleated iPP blends depends on the kinds of compatibilizer. Addition of PP-g-MA significantly reduced the {$\beta$}-crystallization tendency of {$\beta$}-nucleated iPP, while PP-g-GMA, POE-g-MA, and EVA-g-MA have little effect on it. In the compatibilized {$\beta$}-nucleated iPP/PET blends, the blending ways, which controlled the dispersion of {$\beta$}-nucleating agent, influences the {$\beta$}-crystallization tendency intensively. The high {$\beta$}-crystallization tendency and {$\beta$}-crystal content were obtained for compatibilized {$\beta$}-nucleated iPP/PET blends prepared firstly at high temperature and {$\beta$}-nucleating agent added into blends at low temperature; however, the type of compatibilizers has little effect on {$\beta$}-crystallization tendency and melting behavior of blends.},
  langid = {english},
  file = {C\:\\Users\\catas\\Zotero\\storage\\GZTA92E8\\Wang et al. - 2013 - Effect of blending way on β-crystallization tenden.pdf}
}

@article{wang2018,
  title = {New {{Approach}} to {{Optimize Mechanical Properties}} of the {{Immiscible Polypropylene}}/{{Poly}} ({{Ethylene Terephthalate}}) {{Blend}}: {{Effect}} of {{Shish-Kebab}} and {{Core-Shell Structure}}},
