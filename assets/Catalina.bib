@article{espalin2010,
  title = {Fused Deposition Modeling of Patient-specific Polymethylmethacrylate Implants},
  author = {Espalin, David and Arcaute, Karina and Rodriguez, David and Medina, Francisco and Posner, Matthew and Wicker, Ryan},
  editor = {Bourell, Dave and Stucker, Brent},
