}

@article{yesil2013,
  title = {Effect of Carbon Nanotube Surface Treatment on the Morphology, Electrical, and Mechanical Properties of the Microfiber-Reinforced Polyethylene/Poly(Ethylene Terephthalate)/Carbon Nanotube Composites},
