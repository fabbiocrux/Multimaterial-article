}

@article{krcma2022,
  title = {Comparison of the Effects of Multiaxis Printing Strategies on Large-Scale {{3D}} Printed Surface Quality, Accuracy, and Strength},
  author = {Kr{\v c}ma, Martin and Palou{\v s}ek, David},
  year = {2022},
