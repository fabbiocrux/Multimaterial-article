@article{santander2020,
  title = {Closed Loop Supply Chain Network for Local and Distributed Plastic Recycling for {{3D}} Printing: A {{MILP-based}} Optimization Approach},
  shorttitle = {Closed Loop Supply Chain Network for Local and Distributed Plastic Recycling for {{3D}} Printing},
  author = {Santander, Pavlo and Cruz Sanchez, Fabio A. and Boudaoud, Hakim and Camargo, Mauricio},
