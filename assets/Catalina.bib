@article{dorweiler2021,
  title = {Quality {{Control}} in {{3D Printing}}: {{Accuracy Analysis}} of {{3D-Printed Models}} of {{Patient-Specific Anatomy}}},
  shorttitle = {Quality {{Control}} in {{3D Printing}}},
  author = {Dorweiler, Bernhard and Baqu{\'e}, Pia Elisabeth and Chaban, Rayan and Ghazy, Ahmed and Salem, Oroa},
  year = {2021},
