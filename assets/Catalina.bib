@article{bataineh2020,
  title = {Life-{{Cycle Assessment}} of {{Recycling Postconsumer High-Density Polyethylene}} and {{Polyethylene Terephthalate}}},
  author = {Bataineh, Khaled M.},
  year = {2020},
  month = mar,
  journal = {Advances in Civil Engineering},
