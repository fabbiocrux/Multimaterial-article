@article{roosen2020,
  title = {Detailed {{Analysis}} of the {{Composition}} of {{Selected Plastic Packaging Waste Products}} and {{Its Implications}} for {{Mechanical}} and {{Thermochemical Recycling}}},
  author = {Roosen, Martijn and Mys, Nicolas and Kusenberg, Marvin and Billen, Pieter and Dumoulin, Ann and Dewulf, Jo and Van Geem, Kevin M. and Ragaert, Kim and De Meester, Steven},
