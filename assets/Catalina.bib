}

@article{turner2015,
  title = {A Review of Melt Extrusion Additive Manufacturing Processes: {{II}}. {{Materials}}, Dimensional Accuracy, and Surface Roughness},
  shorttitle = {A Review of Melt Extrusion Additive Manufacturing Processes},
  author = {Turner, Brian N. and Gold, Scott A},
