  shorttitle = {{{3D-Printed PCL}}/{{PLA Composite Stents}}},
  author = {Guerra, Antonio J. and Cano, Paula and Rabionet, Marc and Puig, Teresa and Ciurana, Joaquim},
  year = {2018},
