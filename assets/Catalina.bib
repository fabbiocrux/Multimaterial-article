@article{yan2018,
  title = {A {{Review}} of {{3D Printing Technology}} for {{Medical Applications}}},
  author = {Yan, Qian and Dong, Hanhua and Su, Jin and Han, Jianhua and Song, Bo and Wei, Qingsong and Shi, Yusheng},
  year = {2018},
