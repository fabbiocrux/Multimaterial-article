@article{cheng2020,
  title = {{{4D}} Printing of Shape Memory Aliphatic Copolyester via {{UV-assisted FDM}} Strategy for Medical Protective Devices},
  author = {Cheng, Chuan-Ying and Xie, Hui and Xu, Zhi-yuan and Li, Lu and Jiang, Mei-Na and Tang, Li and Yang, Ke-Ke and Wang, Yu-Zhong},
