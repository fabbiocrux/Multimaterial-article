@article{oblom2019,
  title = {{{3D-Printed Isoniazid Tablets}} for the {{Treatment}} and {{Prevention}} of {{Tuberculosis}}\textemdash{{Personalized Dosing}} and {{Drug Release}}},
  author = {{\"O}blom, Heidi and Zhang, Jiaxiang and Pimparade, Manjeet and Speer, Isabell and Preis, Maren and Repka, Michael and Sandler, Niklas},
