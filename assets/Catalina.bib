  file = {C\:\\Users\\catas\\Zotero\\storage\\FWRLXWUM\\Xanthos et al. - 2001 - Reactive modification of polyethylene terephthalat.pdf;C\:\\Users\\catas\\Zotero\\storage\\BSC2GUEU\\pen.html}
