}

@article{colorado2020,
  title = {Sustainability of Additive Manufacturing: The Circular Economy of Materials and Environmental Perspectives},
  shorttitle = {Sustainability of Additive Manufacturing},
  author = {Colorado, Henry A. and Vel{\'a}squez, Elkin I. Guti{\'e}rrez and Monteiro, Sergio Neves},
  year = {2020},
